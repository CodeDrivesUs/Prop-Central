    @RenderSection("scripts", required: false)

    <script>
        var currentpage;
        var totalpages;
        var keyword = $("#keyword").val();
        $(document).ready(function () {
            getData(1);
            currentpage = 1;
            console.log("Loaded for the first time");

        });

        function getData(page) {
            currentpage = page;
            console.log("fetch some JSON data");
            $.ajax({
                url: "/Home/Pagination/?page=" + page + "&keyword=" + keyword, success: function (result) {
                    console.log(result);
                    $("#flatlist").html(result.PaginationList);
                    $("#pagination").html(result.navigation);
                }
            }
            );
        }

        //Overlay script
        $("#_book-btn").click(function () {
            var _html_template = $("#_overlay-template").html();
            $("#_overlay").html(_html_template);
            $("#_main").hide(900);
            console.log("Arrived!!");
        }
        );
        $("#_close-overlay-btn").click(function () {
            $("#_main").show(900);
            console.log("Arrived!!");
        }
        );

        $("#img-check").click(function () {
            $(this).toggleClass("check");
            console.log("Radion button checked");
        });

    </script>
    <script>
        $('.form-control').each(function () {
            floatedLabel($(this));
        });

        $('.form-control').on('input', function () {
            console.log("Test the script");
            floatedLabel($(this));
        });

        function floatedLabel(input) {
            var $field = input.closest('.form-group');
            if (input.val()) {
                $field.addClass('input-not-empty');
            } else {
                $field.removeClass('input-not-empty');
            }
        }
    </script>

