            })

        })
        var Filter = function () {
            var range = $("#amount").val();
            var order = $("#Orderprice").val();
            $.ajax({
                type: "post",
                url: "/Home/Filter/?range=" + range + "&&order=" + order,
                error: function () {
                    alert("error");
                },
                success: function (response) {
                    $('#exampleModalfilter').modal('hide')
                    $("#Result").html(response);


                }
            })
        }
    </script>
