    <script>

        $(document).ready(function () {

            $("#City").change(function () {
                $('#exampleModalfilter').modal('show')

                var City = $(this).val();

                $.ajax({
                    url: "/Home/GetSubcatList/?city=" + City,
                    contentType: "html",
